<mvc:View controllerName="hodek.gateapps.controller.GEPlantSto"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:svm="sap.ui.comp.smartvariants">
    <!-- <Page id="purchaseOrderpage" title="Vendor Portal" showFooter="true" titleAlignment="Auto" showNavButton="true" navButtonPress="onNavBack"> -->
    <f:DynamicPage id="_IDGenDynamicPage2" headerExpanded="{/headerExpanded}">
        <f:title>
            <f:DynamicPageTitle id="_IDGenDynamicPageTitle1">
                <f:navigationActions>
                    <Button id="_IDGenButton4" text="Create Gate Entry" type="Emphasized" press="onSave"/>
                </f:navigationActions>
                <!-- <f:heading>
                        <Title id="_IDGenTitle4" text="{RoutePoData>/PoHeader/PurchaseOrder}" level="H1"/>
                    </f:heading>
                    <f:expandedContent>
                        <Label id="_IDGenLabel15" text="{=${RoutePoData>/PoHeader/purchaseOrderText}?${RoutePoData>/PoHeader/purchaseOrderText}:'NA'}"/>

                    </f:expandedContent>
                    <f:snappedContent>
                        <Label id="_IDGenLabel16" text="{=${RoutePoData>/PoHeader/purchaseOrderText}?${RoutePoData>/PoHeader/purchaseOrderText}:'NA'}"/>

                    </f:snappedContent> -->
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
            <f:DynamicPageHeader id="_IDGenDynamicPageHeader2">
                <f:content>
                    <!-- <Panel id="idPanelRAPO" class="sapUiContentPadding" headerText="General Data" visible="true"> -->
                    <HBox>
                        <VBox width="20%">
                            <Label text="Plant" required="true"/>

                            <ComboBox id="idDropdownPlant" selectedKey="" width="75%" selectionChange="onSelectPlant" items="{
					    path: '/',
                        sorter: { path: 'Plant' }
                    }">
                                <core:Item key="{Plant}" text="{Plant} - {Plantname}"/>
                            </ComboBox>
                        </VBox>
                        <VBox width="20%">
                            <Label text="Vendor" required="true"/>
                            <Input id="idRAII_Vendor" showValueHelp="true" valueHelpOnly="true" valueHelpRequest="vendorValueHelp" width="75%"/>
                        </VBox>
                        <VBox width="20%">
                            <Label text="Challan Number" required="true"/>
                            <Input id="idRAII_Challan" value="" liveChange="onChangeAmount" maxLength="10" width="75%"/>
                        </VBox>


                        <VBox width="20%">
                            <Label text="Date"/>
                            <Input id="idRAII_Date" enabled="false" width="75%"/>
                        </VBox>
                        <VBox width="20%">
                            <Label text="Time"/>
                            <Input id="idRAII_Time" enabled="false" width="75%"/>
                        </VBox>
                    </HBox>
                    <HBox class="sapUiTinyMarginTop">
                        <VBox width="20%">
                            <Label text="Invoice Number" required="true"/>
                            <Input id="idRAII_DocInvNo" value="" change="onChangeRAII_DocInvNo" maxLength="16" width="75%"/>
                        </VBox>
                        <VBox width="20%">
                            <Label text="Invoice Date" required="true"/>
                            <DatePicker id="idRAII_InvDate" width="75%" placeholder="Select Date" change="handleChange_RAPO_InvDate" displayFormat="dd-MM-yyyy" enabled="true" class="sapUiSmallMarginBottom"/>
                        </VBox>
                        <VBox width="20%">
                            <Label text="Eway Bill Number" required="true"/>
                            <Input id="idRAII_EwayNo" maxLength="50" width="75%"/>
                        </VBox>
                        <VBox width="20%">
                            <Label text="Eway Bill Date" required="true"/>
                            <DatePicker id="idRAII_EwayDate" width="75%" placeholder="Select Date" displayFormat="dd-MM-yyyy" enabled="true" />
                        </VBox>
                        <VBox width="20%">
                            <Label text="Transporter" required="true"/>
                            <Input id="idRAII_Trasporter" maxLength="50" width="75%"/>
                        </VBox>


                    </HBox>
                    <HBox class="sapUiTinyMarginTop">
                        <VBox width="20%">
                            <Label text="Amount" required="true"/>
                            <!-- <Input id="idRAII_Amount" liveChange="onChangeAmount" type="Number" width="75%"/> -->
                            <Input id="idRAII_Amount" type="Number" width="75%"/>
                        </VBox>
                        <VBox width="20%">
                            <Label text="Vehical Number" required="true"/>
                            <Input id="idRAII_VehicalNo" maxLength="10" width="75%"/>
                        </VBox>
                        <VBox width="20%">
                            <Label text="Vehical Type" required="true"/>
                            <Input id="idRAII_VehicalType" maxLength="10" width="75%"/>
                        </VBox>
                        <VBox width="20%">
                            <Label text="Vehical Capacity" required="true"/>
                            <Input id="idRAII_VehicalCapacity" type="Number" maxLength="10" width="75%"/>
                        </VBox>
                        <VBox width="20%" visible="false">
                            <Label text="Fiscal Year"/>
                            <Input id="idRAII_FiscalYear" value="" enabled="false" width="75%"/>
                        </VBox>
                        <VBox width="20%">
                            <Label text="Transporter Code" required="true"/>
                            <Input id="idRAII_TrasporterCode" maxLength="50" width="75%"/>
                        </VBox>
                        <!-- <VBox width="20%">
                            <Label text="LR Date"/>
                            <DatePicker id="idRAII_LR_Date" width="75%" placeholder="Select Date" change="handleChange_RAPO_LRDate" displayFormat="dd-MM-yyyy" enabled="true" class="sapUiSmallMarginBottom"/>
                        </VBox>
                        <VBox width="20%">
                            <Label text="LR Number"/>
                            <Input id="idRAII_LR_No" maxLength="25" width="75%"/>
                        </VBox> -->
                    </HBox>


                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <f:content>
            <VBox>
                <Table id="idTable_RAII" items="{path: '/'}" sticky="ColumnHeaders,HeaderToolbar">
                    <headerToolbar>
                        <OverflowToolbar>
                            <!-- <Title text="My Table"/> -->
                            <ToolbarSpacer/>
                            <Button icon="sap-icon://add" text="Add" press="onAddReceiptAsItIsItem"/>
                        </OverflowToolbar>
                    </headerToolbar>
                    <columns>
                        <Column>
                            <Text text="Item No" />
                        </Column>
                        <Column>
                            <Text text="Material" />
                        </Column>
                        <Column>
                            <Text text="Material Description" />
                        </Column>
                        <Column >
                            <Text text="Quantity" />
                        </Column>
                        <Column>
                            <Text text="Unit" />
                        </Column>
                        <Column>
                            <Text text="Action" />
                        </Column>
                    </columns>
                    <items>
                        <ColumnListItem>
                            <cells>
                                <Text text="{Itemno}"/>
                                <Input value="{Product}" showValueHelp="true" valueHelpOnly="true" valueHelpRequest="materialValueHelp"/>
                                <Input value="{ProductDescription}" enabled="false"/>
                                <Input value="{AvailableQuantity}" liveChange="onChangeAmount" />
                                <Text text="{BaseUnit}" />
                                <Button icon="sap-icon://delete" press="onDeleteReceiptAsItIsItem" visible="{= ${Itemno} !== '10' }"></Button>
                            </cells>
                        </ColumnListItem>
                    </items>
                </Table>
                <VBox id="idVBox_QRCode" visible="false">

                </VBox>
            </VBox>
        </f:content>
    </f:DynamicPage>
    <!-- </Page> -->
</mvc:View>