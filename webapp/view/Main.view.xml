<mvc:View controllerName="hodek.gateapps.controller.Main"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m">
	<App id="_IDGenApp">
		<pages >
			<Page id="idMain__Page" enableScrolling="false" title="{i18n>mainPagetitle}" class="MainPage" showHeader="true" showNavButton="true" navButtonPress="onNavBack">

				<content>
					<Button id="_IDGenButton21" icon="sap-icon://nav-back" type="Transparent" press="onNavBack" ariaLabelledBy="editButtonLabel"/>
					<IconTabBar id="idIconTabBarMain" applyContentPadding="false" selectedKey="RouteGEAsn" expandable="false" headerBackgroundDesign="Solid" select="onSelectTabItem" stretchContentHeight="true" class="ICTBCustom">
						<items>
							<IconTabFilter id="_IDGenIconTabFilter1" text="Gate Entry With ASN" key="RouteGEAsn"/>
							<IconTabFilter id="_IDGenIconTabFilter2" text="Gate Entry w/o ASN" key="RouteGEWasn" />
							<IconTabFilter id="_IDGenIconTabFilter"  text="In Plant STO" key="RouteGEPlantSto" />
							<IconTabFilter id="_IDGenIconTabFilter4"  text="Receipt As Is" key="RouteGEAsItIs" />
							<IconTabFilter id="_IDGenIconTabFilter5" text="Reprint Gate Entry" key="RouteGEReprint" />
							<IconTabFilter id="_IDGenIconTabFilter6" text="Cancel Gate Entry" key="RouteCancelGE" />
						</items>
						<content>
							<NavContainer id="Main__NavContainer" height="100%" width="100%"/>
						</content>
					</IconTabBar>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>