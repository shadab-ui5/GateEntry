<mvc:View controllerName="hodek.gateapps.controller.GEAsnDetail"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:fb="sap.ui.comp.filterbar"
    xmlns:core="sap.ui.core"
    xmlns:u="sap.ui.unified"
    xmlns="sap.m"
    xmlns:f="sap.f"
    xmlns:upload="sap.m.upload"
    xmlns:svm="sap.ui.comp.smartvariants">
    <!-- <Page id="asnCreationpage" title="{i18n>title}" showFooter="true" titleAlignment="Auto" showNavButton="true" navButtonPress="onNavBack"> -->
    <f:DynamicPage id="_IDGenDynamicPage3" stickySubheaderProvider="iconTabBar" headerExpanded="{/headerExpanded}" showFooter="true" fitContent="true">
        <f:title>
            <f:DynamicPageTitle id="_IDGenDynamicPageTitle2">
                <f:actions>
                    <Button id="_IDGenButton9" type="Emphasized" text="Create Gate Entry" press="onSave" class="sapUiSmallMarginEnd"/>
                </f:actions>
                <f:heading>
                    <Title id="_IDGenTitle1" text="{AsnHeaderModel>/Ponumber}" level="H1"/>
                </f:heading>
                <f:expandedContent>
                    <HBox id="_IDGenHBox4" width="100%" justifyContent="SpaceBetween">
                        <Label id="_IDGenLabel14" text="{=${AsnHeaderModel>/PonumberText}?${AsnHeaderModel>/PonumberText}:'NA'}"/>
                    </HBox>
                </f:expandedContent>
                <f:snappedContent>
                    <Label id="_IDGenLabel22" text="{=${AsnHeaderModel>/PonumberText}?${AsnHeaderModel>/PonumberText}:'NA'}"/>

                </f:snappedContent>
            </f:DynamicPageTitle>
        </f:title>
        <f:header>
            <f:DynamicPageHeader id="_IDGenDynamicPageHeader2">
                <f:content>
                    <HBox id="_IDGenHBox5" class="sapUiSmallMarginBottom">
                        <VBox id="_IDGenVBox5" width="20%">
                            <Label id="_IDGenLabel5" text="Invoice Number" required="true"/>
                            <Text id="idDocInvNo" text="{AsnHeaderModel>/InvoiceNo}" width="75%"/>
                        </VBox>
                        <VBox id="_IDGenVBox20" width="20%">
                            <Label id="_IDGenLabel26" text="Plant" required="true"/>
                            <Text id="idDropdownPlant" text="{AsnHeaderModel>/Plant}" width="75%"/>
                        </VBox>
                        <VBox id="_IDGenVBox23" width="20%">
                            <Label id="_IDGenLabel25" text="Purchase Order" required="true"/>
                            <Text id="idRAPO_PO_Order" text="{AsnHeaderModel>/Ponumber}" width="75%"/>
                        </VBox>
                        <VBox id="_IDGenVBox21" width="20%">
                            <Label id="_IDGenLabel23" text="Date"/>
                            <Text id="idRAPO_Date" text="{ path: 'AsnHeaderModel>/SystemDate', formatter: '.formatter.formatDateToYyyyMmDd' }" width="75%" />
                        </VBox>
                        <VBox id="_IDGenVBox22" width="20%">
                            <Label id="_IDGenLabel24" text="Time"/>
                            <Text id="idRAPO_Time" text="{ path:'AsnHeaderModel>/SystemTime',formatter:'.formatter.formatHours'}" width="75%"/>
                        </VBox>
                    </HBox>
                    <HBox id="_IDGenHBox2" class="sapUiSmallMarginBottom">

                        <VBox id="_IDGenVBox6" width="20%">
                            <Label id="_IDGenLabel6" text="Invoice Date" required="true"/>
                            <Text id="idRAPO_InvDate" width="75%" text="{ path: 'AsnHeaderModel>/InvoiceDate', formatter: '.formatter.formatDateToYyyyMmDd' }"/>
                        </VBox>
                        <VBox id="_IDGenVBox7" width="20%">
                            <Label id="_IDGenLabel7" text="Vehical Number" required="true"/>
                            <Text id="idRAPO_VehicalNo" text="{AsnHeaderModel>/Vehicleno}" width="75%"/>
                        </VBox>
                        <VBox id="_IDGenVBox8" width="20%">
                            <Label id="_IDGenLabel8" text="Transporter" required="true"/>
                            <Text id="idRAPO_Trasporter" text="{AsnHeaderModel>/Transporter}" width="75%"/>
                        </VBox>
                        <VBox id="_IDGenVBox10" width="20%">
                            <Label id="_IDGenLabel10" text="Eway Bill Number" required="true"/>
                            <Text id="idRAPO_EwayNo" text="{AsnHeaderModel>/Ewayno}" width="75%"/>
                        </VBox>
                        <VBox id="_IDGenVBox24" width="20%">
                            <Label id="_IDGenLabel29" text="Eway Bill Date"/>
                            <Text id="idRAPO_EWAY_Date" width="75%" text="{ path: 'AsnHeaderModel>/EwaybillDate', formatter: '.formatter.formatDateToYyyyMmDd' }" class="sapUiSmallMarginBottom"/>
                        </VBox>
                    </HBox>

                    <HBox id="_IDGenHBox3">
                        <VBox id="_IDGenVBox9" width="20%">
                            <Label id="_IDGenLabel9" text="Amount" />
                            <Text id="idRAPO_Amount" text="{AsnHeaderModel>/Amount}" width="75%"/>
                        </VBox>
                        <VBox id="_IDGenVBox12" width="20%">
                            <Label id="_IDGenLabel12" text="LR Number"/>
                            <Text id="idRAPO_LR_No" text="{AsnHeaderModel>/Lrnumber}" width="75%"/>
                        </VBox>
                        <VBox id="_IDGenVBox11" width="20%">
                            <Label id="_IDGenLabel11" text="LR Date"/>
                            <Text id="idRAPO_LR_Date" width="75%" text="{ path: 'AsnHeaderModel>/Lrdate', formatter: '.formatter.formatDateToYyyyMmDd' }" class="sapUiSmallMarginBottom"/>
                        </VBox>
                        <VBox id="_IDGenVBox111" width="20%" visible="false">
                            <Label id="_IDGenLabel111" text="Supplier"/>
                            <Text id="idSupplier" text="{AsnHeaderModel>/Vendor}" class="sapUiSmallMarginBottom"/>
                        </VBox>
                    </HBox>

                </f:content>
            </f:DynamicPageHeader>
        </f:header>
        <f:content >

            <IconTabBar id="iconTabBar" class="sapUiNoContentPadding filterbarTabAsn">
                <items>
                    <IconTabFilter id="_IDGenIconTabFilter3" text="Items">
                        <VBox id="_IDGenVBox14">
                            <Table id="idTable_RAPO" alternateRowColors="true" items="{path: 'AsnItemsModel>/'}" sticky="ColumnHeaders,HeaderToolbar">
                                <headerToolbar>
                                    <OverflowToolbar id="_IDGenOverflowToolbar">
                                        <Title id="_IDGenTitle6" text="Items" level="H2"/>
                                        <ToolbarSpacer id="_IDGenToolbarSpacer"/>
                                        <!-- <Button id="_IDGenButton" icon="sap-icon://add" text="Add" press="onAddReceiptAsPOTableItem"/> -->
                                    </OverflowToolbar>
                                </headerToolbar>
                                <columns>
                                    <Column id="_IDGenColumn" visible="false">
                                        <Text id="_IDGenText" text="PO No. / SA No." />
                                    </Column>
                                    <Column id="_IDGenColumn1">
                                        <Text id="_IDGenText1" text="Line Item No" />
                                    </Column>
                                    <Column id="_IDGenColumn3">
                                        <Text id="_IDGenText3" text="Item Code/Material" />
                                    </Column>
                                    <Column id="_IDGenColumn4">
                                        <Text id="_IDGenText4" text="Item Description" />
                                    </Column>
                                    <Column id="_IDGenColumn5">
                                        <Text id="_IDGenText5" text="UOM" />
                                    </Column>
                                    <Column id="_IDGenColumn7">
                                        <Text id="_IDGenText11" text="Quantity" />
                                    </Column>
                                    <Column visible="false">
                                        <Text text="OrderQuantity" />
                                    </Column>
                                    <Column visible="false">
                                        <Text text="postedquantity" />
                                    </Column>
                                </columns>
                                <items>
                                    <ColumnListItem id="_IDGenColumnListItem">
                                        <cells>
                                            <Text id="_IDGenText18" text="{AsnItemsModel>Ponumber}" />
                                            <Text id="_IDGenText15" text="{AsnItemsModel>LineItem}"/>
                                            <Text id="_IDGenText16" text="{AsnItemsModel>Material}"/>
                                            <Text id="_IDGenText17" text="{AsnItemsModel>Materialdesc}"/>
                                            <!-- <Text id="_IDGenText" value="{Material}" showValueHelp="true" valueHelpOnly="true" valueHelpRequest="material_RAPOValueHelp"/> -->
                                            <Text id="_" text="{AsnItemsModel>BaseUnit}" />
                                            <Text id="idInpRAPOItemQuantity" text="{AsnItemsModel>Postedquantity}" />
                                            <Text text="{AsnItemsModel>Uom}" />
                                            <Text text="{AsnItemsModel>postedquantity}" />
                                        </cells>
                                    </ColumnListItem>
                                </items>
                            </Table>
                            <VBox id="idVBox_QRCode" visible="false">

                            </VBox>
                        </VBox>
                    </IconTabFilter>
                </items>
            </IconTabBar>
        </f:content>
    </f:DynamicPage>
    <!-- </Page> -->
</mvc:View>